{
  "name": "ronvalstarnl",
  "author": "Ron Valstar",
  "version": "0.5.21",
  "license": "MIT",
  "repository": "github:sjeiti/ronvalstarnl",
  "description": "ste",
  "scripts": {
    "start": "run-p build serve",
    "build": "run-s clean build:less \"webpack -- --env development --watch\"",
    "build:prod": "run-s clean build:less build:unzip build:json build:search build:icons \"webpack -- --env production\" build:sitemap build:rss",
    "build:unzip": "unzip src/data/icomoon.zip temp/icomoon",
    "build:json": "node task/markdown2json",
    "build:search": "node task/buildSearch",
    "build:sitemap": "node task/buildSitemap --target dist",
    "build:rss": "node task/buildRSS --target dist",
    "build:less": "node task/lessGlob",
    "build:icons": "node task/icons --source temp/icomoon/SVG/logo.svg --target ./dist/static/icon --sizes 16,32,180",
    "write": "chokidar \"src/data/markdown/*.md\" -c \"npm run build:json\"",
    "write:future": "grep -rlw 9999 src/data/markdown/*",
    "write:futurew": "findstr 9999 src/data/markdown/* 2>nul",
    "webpack": "npx webpack --config webpack.config.js",
    "clean": "del-cli dist -f",
    "serve": "node task/serve dist 7047",
    "serve:temp": "node task/serve temp 7777",
    "lint:fix": "eslint src/js/**/*.js --fix",
    "version": "next-version package.json",
    "version:minor": "npm run version -- --minor",
    "version:major": "npm run version -- --major",
    "build:test": "npx webpack --config webpack.test.config.js --env.development",
    "build:test:watch": "npm run build:test & chokidar \"src/**/*.*\" -c \"npm run build:test\"",
    "__build:prerender": "node task/preRender --entry http://localhost:7047 --target dist",
    "__build:post": "node task/postBuild"
  },
  "dependencies": {
    "@emmetio/expand-abbreviation": "^0.7.3",
    "Experiments": "git+https://github.com/Sjeiti/experiment.git",
    "gsap": "^3.6.0",
    "prismjs": "^1.23.0",
    "signals": "^1.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.13",
    "@babel/plugin-proposal-class-properties": "^7.12.13",
    "@babel/plugin-proposal-decorators": "^7.12.13",
    "@babel/plugin-proposal-do-expressions": "^7.12.13",
    "@babel/plugin-proposal-export-default-from": "^7.12.13",
    "@babel/plugin-proposal-export-namespace-from": "^7.12.13",
    "@babel/plugin-proposal-function-bind": "^7.12.13",
    "@babel/plugin-proposal-function-sent": "^7.12.13",
    "@babel/plugin-proposal-json-strings": "^7.12.13",
    "@babel/plugin-proposal-logical-assignment-operators": "^7.12.13",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.12.13",
    "@babel/plugin-proposal-numeric-separator": "^7.12.13",
    "@babel/plugin-proposal-optional-chaining": "^7.12.13",
    "@babel/plugin-proposal-pipeline-operator": "^7.12.13",
    "@babel/plugin-proposal-throw-expressions": "^7.12.13",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-syntax-import-meta": "^7.10.4",
    "@babel/preset-env": "^7.12.13",
    "adm-zip-cli": "^1.0.0",
    "babel-eslint": "^9.0.0",
    "babel-loader": "^8.2.2",
    "babel-preset-babili": "0.0.12",
    "babel-preset-minify": "^0.5.1",
    "body-parser": "^1.19.0",
    "chokidar-cli": "^2.1.0",
    "commander": "^7.2.0",
    "copy-webpack-plugin": "^7.0.0",
    "css-loader": "^5.0.1",
    "css-properties-values": "^1.0.0",
    "del-cli": "^3.0.1",
    "eslint": "^7.19.0",
    "express": "^4.17.1",
    "file-loader": "^6.2.0",
    "glob": "^7.1.6",
    "html-loader": "^1.3.2",
    "html-minifier": "^4.0.0",
    "less": "^4.1.1",
    "less-loader": "^8.0.0",
    "marked": "^2.0.0",
    "mkdirp": "^1.0.4",
    "next-version": "^0.11.7",
    "node-fetch": "^2.6.1",
    "node-html-parser": "^2.1.0",
    "npm-run-all": "^4.1.5",
    "open": "^7.4.0",
    "raw-loader": "^4.0.2",
    "rimraf": "^3.0.2",
    "serve-static": "^1.14.1",
    "sharp": "^0.29.1",
    "style-loader": "^2.0.0",
    "url-loader": "^4.1.1",
    "webpack": "^5.21.1",
    "webpack-cli": "^4.5.0",
    "webpack-watch-files-plugin": "^1.1.0",
    "yorkie": "^2.0.0"
  },
  "optionalDependencies": {
    "puppeteer": "^7.0.1"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10"
  ],
  "gitHooks": {
    "pre-commit": "npm run version"
  },
  "engines": {
    "node": "^14.15.1",
    "npm": "^6.14.9"
  }
}
